{
    "name": "laravel/lumen",
    "description": "Angular 2. Lumen. Integrated.",
    "keywords": ["framework", "laravel", "lumen", "angular2"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
        "league/flysystem": "^1.0",
        "dingo/api": "1.0.x@dev",
        "tymon/jwt-auth": "1.0.0-alpha.2",
        "barryvdh/laravel-cors": "^0.8.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    }
}
